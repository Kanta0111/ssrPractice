{
  "name": "ssrpractice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:all": "npm run build && npm run test && npm run test:puppeteer:headless",
    "test:puppeteer": "HEADLESS=false JEST_PUPPETEER_CONFIG=.puppeteer/jest-puppeteer.config.js jest --config .puppeteer/jest.config.puppeteer.js",
    "test:puppeteer:headless": "HEADLESS=true JEST_PUPPETEER_CONFIG=.puppeteer/jest-puppeteer.config.js jest --config .puppeteer/jest.config.puppeteer.js",
    "build": "webpack --config webpack.config.js --mode=production && webpack --config webpack.config.bff.js --mode=production && npm run copy",
    "build:analyze": "ANALYZE=true webpack --config webpack.config.js --mode=production && webpack --config webpack.config.bff.js --mode=production && npm run copy",
    "build:bff": "webpack --config webpack.config.bff.js --mode=production && npm run copy",
    "dev": "webpack --config webpack.config.js --mode development --watch & webpack --config webpack.config.bff.js --mode development --watch & npx nodemon dist/server.js",
    "dev:front": "webpack --config webpack.config.js --mode development --watch & node dist/server.js",
    "dev:functions": "FIREBASE_ENV=development npm run dev",
    "copy": "cp -Rp static dist/public/",
    "start": "node dist/server.js",
    "lint": "npx eslint src bff",
    "lint:fix": "npx eslint --fix src bff",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
